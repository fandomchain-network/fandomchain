#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/abi-wan-kanabi@2.2.4/node_modules/abi-wan-kanabi/dist/node_modules:/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/abi-wan-kanabi@2.2.4/node_modules/abi-wan-kanabi/node_modules:/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/abi-wan-kanabi@2.2.4/node_modules:/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/abi-wan-kanabi@2.2.4/node_modules/abi-wan-kanabi/dist/node_modules:/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/abi-wan-kanabi@2.2.4/node_modules/abi-wan-kanabi/node_modules:/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/abi-wan-kanabi@2.2.4/node_modules:/home/berthier/Desktop/Projects/fandomChain/ts-client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../abi-wan-kanabi/dist/generate.js" "$@"
else
  exec node  "$basedir/../abi-wan-kanabi/dist/generate.js" "$@"
fi
